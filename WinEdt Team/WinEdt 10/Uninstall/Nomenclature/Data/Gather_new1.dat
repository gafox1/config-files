    ACTION="Find"
      IMAGE="Find"
      MACRO="GlobalMark(1);GDITrack;"
    ACTION="Insert"
      IMAGE="Insert"
      MACRO="ActivateChild;BeginGroup;SetSel(0);Ins('%?');EndGroup;"
